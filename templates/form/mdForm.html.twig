{# Customize form protype to material design bootstrap#}
{%- block form_row -%}
    {% spaceless %}
        <div class="md-form">
            {{- form_label(form) -}}
            {{- form_widget(form,{'attr': {'class': 'form-control'}}) -}}
            {{- form_errors(form) -}}
        </div>
    {% endspaceless %}
{%- endblock form_row -%}